{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Julep",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "theme": "quill",
  "background": {
    "style": "gradient"
  },
  "colors": {
    "primary": "#004E64",
    "light": "#00A5CF",
    "dark": "#25A18E",
    "background": {
      "dark": "#072b3f",
      "light": "#F7FCF7"
    },
    "anchors": {
      "from": "#25A18E",
      "to": "#7AE582"
    }
  },
  "layout": "topnav",
  "font": {
    "headings": {
      "family": "Instrument Sans",
      "weight": 500
    },
    "body": {
      "family": "Figtree",
      "weight": 500
    }
  },
  "rounded": "sharp",
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "codeBlock": {
    "mode": "dark"
  },
  "sidebar": {
    "items": "card"
  },
  "topbar": {
    "style": "gradient"
  },
  "search": {
    "location": "top",
    "prompt": "Search Julep docs"
  },
  "versions": ["v1", "dev"],
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:developers@julep.ai"
    }
  ],
